{
  "name": "environment-variable-injector",
  "version": "0.0.1",
  "description": "Environment-variable-injector plugin for release-it",
  "main": "index.js",
  "scripts": {
    "test": "bron test.js",
    "release": "release-it"
  },
  "keywords": [
    "release",
    "release-it",
    "release-it-plugin",
    "environment-variables"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/volvofinans/environment-variable-injector.git"
  },
  "homepage": "https://github.com/volvofinans/environment-variable-injector#readme",
  "bugs": "https://github.com/volvofinans/environment-variable-injector/issues",
  "author": {
    "name": "Johan Hammar",
    "email": "johan.hammar@volvofinans.se"
  },
  "peerDependencies": {
    "release-it": "^15.0.0"
  },
  "devDependencies": {
    "bron": "^2.0.3",
    "release-it": "^15.0.0",
    "@release-it/keep-a-changelog": "^3.0.0",
    "sinon": "^14.0.0"
  },
  "engines": {
    "node": ">=16"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "hooks": {
      "before:init": "npm test"
    },
    "plugins": {
      "@release-it/keep-a-changelog": {
        "filename": "CHANGELOG.md",
        "addUnreleased": true
      }
    }
  }
}